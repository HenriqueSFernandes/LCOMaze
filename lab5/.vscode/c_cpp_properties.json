{
    "env": {
        "minix.include": "${workspaceFolder}/../.minix-src/include"
    },
    "configurations": [
        {
            "name": "linux-gcc-x64",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "${default}",
            "defines": [
                "__minix",
                "_MINIX",
                "_NETBSD_SOURCE",
                "__i386__"
            ],
            "includePath": [
                "${workspaceFolder}/**",
                "/home/ricky/Desktop/LCOM/MINIX-LCOM/shared/g1/.minix-src/include"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${env:minix.include}"
                ],
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": ""
            },
            "cppStandard": "${default}",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}